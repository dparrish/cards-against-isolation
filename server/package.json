{
  "name": "cards-against-isolation-server",
  "version": "0.0.1",
  "scripts": {
    "gulp": "gulp",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "gulp build"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/socket.io": "^2.1.4",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
